<default>
  <leaf>
    <fnames>
      <!-- photorespiratory compensation point, gamma star; farquhar 1980 calculate from Kc and Ko, 
      f_gstar_constref will get a specified value at 25 and scale according to gstar_tcor, f_gstar_c1991 calculates from tau the CO2:O2 specificity ratio -->
      <gstar>c('f_gstar_constant','f_gstar_f1980','f_gstar_constref','f_gstar_c1991')</gstar>
      <!-- temperature scaling of gamma star, only used if <gstar> is set to 'f_gstar_constref' -->
      <gstar_tcor>'f_temp_scalar_quadratic_bf1985'</gstar_tcor>
      <!-- temperature scaling of tau, only used if <gstar> is set to 'f_gstar_c1991' -->
      <tau_tcor>'f_temp_scalar_Q10'</tau_tcor>
      <!-- temperature scaling of Kc -->
      <Kc_tcor>'f_temp_scalar_Arrhenius'</Kc_tcor>
      <!-- temperature scaling of Ko -->
      <Ko_tcor>'f_temp_scalar_Arrhenius'</Ko_tcor>
      <!-- scaling of Vcmax at reftemp.rd -->
      <vcmax>c('f_vcmax_constant','f_vcmax_lin','f_vcmax_clm')</vcmax>
      <!-- scaling of Jmax at reftemp.rd, f(Vcmax), f_jmax_lin_t allows for temperature scaling of JV ratio based on Kattge & Knorr 2007 -->
      <jmax>c('f_jmax_constant','f_jmax_walker2014','f_jmax_lin','f_jmax_lin_t)</jmax>
      <!-- scaling of TPU at reftemp.rd, f(Vcmax) -->
      <tpu>c('f_tpu_constant','f_tpu_lin')</tpu>
      <!-- increasing component of temperature scaling of Vcmax and Jmax -->
      <vcmax_tcor_asc>c('f_scalar_none','f_temp_scalar_Q10','f_temp_scalar_Arrhenius')</vcmax_tcor_asc>
      <jmax_tcor_asc>c('f_scalar_none','f_temp_scalar_Q10','f_temp_scalar_Arrhenius')</jmax_tcor_asc>
      <!-- decreasing component of temperature scaling of Vcmax and Jmax -->
      <vcmax_tcor_des>c('f_temp_scalar_modArrhenius_des','f_temp_scalar_collatz1991_des','f_temp_scalar_cox2001_des')</vcmax_tcor_des>
      <jmax_tcor_des>c('f_temp_scalar_modArrhenius_des','f_temp_scalar_collatz1991_des','f_temp_scalar_cox2001_des')</jmax_tcor_des>
      <!-- is temperature scaling of TPU the same as that of Vcmax or not -->
      <tpu_tcor_dependence>'f_tpu_tcor_dependent'</tpu_tcor_dependence>
      <!-- increasing component of temperature scaling of TPU, if independent of Vcmax -->
      <tpu_tcor_asc>c('f_scalar_none','f_temp_scalar_Q10','f_temp_scalar_Arrhenius')</tpu_tcor_asc>
      <!-- decreasing component of temperature scaling of TPU, if independent of Vcmax -->
      <tpu_tcor_des>c('f_temp_scalar_modArrhenius_des','f_temp_scalar_collatz1991_des','f_temp_scalar_cox2001_des')</tpu_tcor_des>
      <!-- function to determine delta S (entropy term) for rd, vcmax, & jmax; can be used for temperature acclimation -->
      <deltaS>'f_deltaS'</deltaS>
      <!-- electron transport -->
      <etrans>c('f_j_farquharwong1984','f_j_harley1992','f_j_collatz1991','f_j_farquhar1980')</etrans>
      <!-- RuBP saturated gross carboxylation rate -->
      <Acg>'f_Acg_farquhar1980'</Acg>
      <!-- RuBP limited gross carboxylation rate -->
      <Ajg>'f_Ajg_generic'</Ajg>
      <!-- TPU limited gross carboxylation rate, mostly all variants of this are special cases of that presented in von Caemmerer 2000 -->
      <Apg>c('f_Apg_vonc2000','f_Apg_foley1996')</Apg>
      <!-- gas diffusion function, Fick's law; added a zero bound when testing the solver but I think this is unnecessary -->
      <gas_diff>c('f_ficks_ci','f_ficks_ci_bound0')</gas_diff>
      <!-- scaling of Rd at reftemp.rd, f(Vcmax), f(N), -->
      <respiration>c('f_rd_constant','f_rd_lin_vcmax','f_rd_lin_N')</respiration>
      <!-- scaling of Rd in the dark to rd in the light, default is fixed ratio with ratio set to 1 -->
      <rl_rd_scalar>c('f_rl_rd_fixed','f_rl_rd_lloyd1995')</rl_rd_scalar>
      <!-- is temperature scaling of Rd the same as that of Vcmax or not -->
      <rd_tcor_dependence>c('f_rd_tcor_dependent','f_rd_tcor_independent')</rd_tcor_dependence> 
      <!-- increasing component of temperature scaling of Rd, if independent of Vcmax -->
      <rd_tcor_asc>c('f_scalar_none','f_temp_scalar_Q10','f_temp_scalar_Arrhenius')</rd_tcor_asc>
      <!-- decreasing component of temperature scaling of Rd, if independent of Vcmax -->
      <rd_tcor_des>c('f_temp_scalar_modArrhenius_des','f_temp_scalar_collatz1991_des','f_temp_scalar_cox2001_des')</rd_tcor_des>
      <!-- function to determine Q10 for rd, vcmax, & jmax; can be used for temperature acclimation -->
      <q10_func.rd>c('f_q10_constant','f_q10_lin_t')</q10_func.rd>          
      <q10_func.vcmax>'f_q10_constant'</q10_func.vcmax>          
      <q10_func.jmax>'f_q10_constant'</q10_func.jmax> 
      <!-- fresh weight dry weight ratio of Sphagnum -->
      <fwdw_ratio>'f_none'</fwdw_ratio>
      <!-- Ci:Ca ratio, only used if <rs> is '' -->
      <cica_ratio>'f_cica_constant'</cica_ratio>  
      <!-- internal resistance to CO2 transport -->
      <ri>c('f_r_zero','f_ri_constant')</ri>
      <!-- stomatal resistance to H2O & CO2 transport, constant CiCa is implicit in LPJ and Cox 1999 does something similar for JULES -->
      <rs>c('f_r_zero','f_rs_ball1987','f_rs_leuning1995','f_rs_medlyn2011','f_rs_constantCiCa','f_rs_cox1998')</rs>
      <!-- leaf boundary layer resistance to H2O & CO2 transport -->
      <rb>c('f_r_zero','f_rb_constant')</rb>
      <!-- solver, numerical or analytical -->
      <solver>c('f_R_Brent_solver','f_A_r_leaf_analytical')</solver>
      <!-- function passed to solver -->
      <solver_func>'f_A_r_leaf'</solver_func>
      <!-- selection of limiting rate function, straight miniimum Farquhar 1980 or smoothed Collatz 1991 -->
      <Alim>c('f_lim_farquhar1980','f_lim_collatz1991')</Alim>
    </fnames>
    <pars>
      <!-- output diagnostics like transition point and non-stomatal limited photosynthesis, T/F -->
      <diag>F</diag>
      <!-- fraction of light absorbed by leaf that is absorbed by photosystems, 0-1 -->
      <a>0.8</a>
      <!-- fraction of incident light transmitted through leaf, 0-1 -->
      <f>0.23</f>
      <!-- ratio of RuBisCO turnover numbers for oxgenation and carboxylation (unitless) -->
      <ko_kc_ratio>0.21</ko_kc_ratio>       
      <!-- curvature of farquhar and wong 1984 electron transport, 0-1 --> 
      <theta_j>0.9</theta_j>
      <!-- curvature of collatz 1991 limiting rate selection function, theta in Collatz notation, Acg or Ajg, 0-1 --> 
      <theta_col_cj>0.95</theta_col_cj>
      <!-- curvature of collatz 1991 limiting rate selection function, beta in Collatz notation, (Acg or Ajg) or Apg, 0-1 --> 
      <theta_col_cjp>0.95</theta_col_cjp>
      <!-- intercept & slope of Vcmax25 to leaf N area relationship -->
      <avn_25>10</avn_25>
      <bvn_25>30</bvn_25>
      <!-- intercept & slope of Jmax25 to Vcmax25 area relationship -->
      <ajv_25>29</ajv_25>
      <bjv_25>1.63</bjv_25>
      <!-- intercept & slope of Jmax25:Vcmax25 ratio to leaf temperature relationship -->
      <a_jvt_25>2.59</a_jvt_25>      
      <b_jvt_25>0.035</b_jvt_25>      
      <!-- normalisation constant & exponent of Jmax25 to Vcmax25 area relationship -->
      <e_ajv_25>1.01</e_ajv_25>
      <e_bjv_25>0.89</e_bjv_25>
      <!-- intercept & slope of TPU25 to Vcmax25 area relationship, intercept of zero slope of 0.16667 is equivalent to Collatz 1991 Vcmax/2 but in von Caemmerer's notation  -->
      <atv_25>0</atv_25>
      <btv_25>0.16667</btv_25>
      <!-- fraction of leaf N in RuBisCO -->
      <flnr>0.09</flnr>
      <!-- RuBisCO molar weight to N weight ratio -->
      <fnr>7.16</fnr>
      <!-- RuBisCO CO2 specific activity -->
      <Rsa>60</Rsa>
      <!-- fraction of phosphate not returned to the chloroplast -->
      <Apg_alpha>0</Apg_alpha>
      <!-- minimum stomatal conductance, mol m-2s-1 -->
      <g0>0.01</g0>
      <!-- stomatal conductance to environment slope parameter -->
      <g1_medlyn>5</g1_medlyn>
      <!-- stomatal conductance to environment slope parameter -->
      <g1_leuning>5</g1_leuning>
      <!-- stomatal conductance to environment vpd normalisation parameter -->
      <d0>2</d0>
      <!-- stomatal conductance to environment slope parameter -->
      <g1_ball>5</g1_ball>
      <!-- constant stomatal resistance -->
      <rs>6.667</rs>
      <!-- constant Ci:Ca ratio -->
      <cica_chi>0.7</cica_chi>
      <!-- constant leaf boundary layer resistance -->
      <rb>0.1</rb>
      <!-- constant internal resistance -->
      <ri>28.5714285714286</ri>
      <!-- diffusivity of CO2 in water -->
      <co2_diff>1.7e-09</co2_diff>
      <!-- ratio of HCO to CO2 in solution -->
      <hco_co2_ratio>0</hco_co2_ratio>
      <!-- ratio of HCO to CO2 diffusivity in water -->
      <hco_co2_diff_ratio>0.56</hco_co2_diff_ratio>
      <!-- intercept & slope of Sphagnum fresh weight dry weight ratio to water table depth -->
      <fwdw_wl_slope>-0.022</fwdw_wl_slope>
      <fwdw_wl_sat>16</fwdw_wl_sat>
      <!-- normalisation constant & exponent of Sphagnum fresh weight dry weight ratio to water table depth -->
      <fwdw_wl_exp_a>-0.037</fwdw_wl_exp_a>
      <fwdw_wl_exp_b>3.254</fwdw_wl_exp_b>
      <!-- intercept & slope of Rd25 to Vcmax25 area relationship -->
      <a_rdv_25>0.0</a_rdv_25>
      <b_rdv_25>0.015</b_rdv_25>
      <!-- intercept & slope of Rd25 to leaf N area relationship -->
      <a_rdn_25>0.5</a_rdn_25>
      <b_rdn_25>0.15</b_rdn_25>
      <!-- ratio of Rd in the light to Rd in the dark -->
      <rl_rd_ratio>1</rl_rd_ratio>
      <!-- parameters for Lloyd 1995 Rd in the light function of Rd in the dark -->
      <rl_rd_lloyd_a>0.5</rl_rd_lloyd_a>
      <rl_rd_lloyd_b>0.05</rl_rd_lloyd_b>
      <!-- reference temperatures for temperature sensitive parameters -->
      <reftemp.rd>25</reftemp.rd>
      <reftemp.vcmax>25</reftemp.vcmax>
      <reftemp.jmax>25</reftemp.jmax>
      <reftemp.tpu>25</reftemp.tpu>
      <reftemp.Kc>25</reftemp.Kc>
      <reftemp.Ko>25</reftemp.Ko>
      <reftemp.gstar>25</reftemp.gstar>
      <reftemp.tau>25</reftemp.tau>
      <!-- constants at reference temperature for temperature sensitive parameters -->
      <atref.rd>1.5</atref.rd>
      <atref.vcmax>70</atref.vcmax>
      <atref.jmax>0</atref.jmax>
      <atref.tpu>10</atref.tpu>
      <atref.Kc>40.49</atref.Kc>
      <atref.Ko>27.84</atref.Ko>
      <atref.gstar>4.325</atref.gstar>
      <atref.tau>2600</atref.tau>
      <atref.vomax>70</atref.vomax>
      <!-- activation energy for temperature sensitive parameters -->
      <Ha.rd>69830</Ha.rd>
      <Ha.vcmax>69830</Ha.vcmax>
      <Ha.jmax>100280</Ha.jmax>
      <Ha.tpu>69830</Ha.tpu>
      <Ha.Kc>79430</Ha.Kc>
      <Ha.Ko>36380</Ha.Ko>
      <Ha.gstar>37830</Ha.gstar>
      <Ha.vomax>60110</Ha.vomax>
      <!-- deactivation energy for temperature sensitive parameters -->
      <Hd.rd>2e+05</Hd.rd>
      <Hd.vcmax>2e+05</Hd.vcmax>
      <Hd.jmax>2e+05</Hd.jmax>
      <Hd.tpu>2e+05</Hd.tpu>
      <!-- temperature optimum for temperature sensitive parameters -->
      <Topt.rd>27.56</Topt.rd>
      <Topt.vcmax>27.56</Topt.vcmax>
      <Topt.jmax>19.89</Topt.jmax>
      <Topt.tpu>27.56</Topt.tpu>
      <!-- delta S for temperature sensitive parameters -->
      <deltaS.vcmax>NA</deltaS.vcmax>
      <deltaS.jmax>NA</deltaS.jmax>
      <deltaS.tpu>NA</deltaS.tpu>
      <!-- intercept and slope of delta S to leaf temperature for temperature sensitive parameters -->
      <a_deltaS_t.vcmax>668</a_deltaS_t.vcmax>
      <a_deltaS_t.jmax>660</a_deltaS_t.jmax>
      <b_deltaS_t.vcmax>-1.07</b_deltaS_t.vcmax>
      <b_deltaS_t.jmax>-0.75</b_deltaS_t.jmax>
      <!-- Q10 for temperature sensitive parameters -->
      <q10.rd>2</q10.rd>
      <q10.vcmax>2</q10.vcmax>
      <q10.jmax>2</q10.jmax>
      <q10.tpu>2</q10.tpu>
      <q10.Kc>2</q10.Kc>
      <q10.Ko>2</q10.Ko>
      <q10.tau>0.57</q10.tau>
      <!-- intercept and slope of delta S to leaf temperature for temperature sensitive parameters -->
      <a_q10_t.rd>3.22</a_q10_t.rd>
      <b_q10_t.rd>-0.046</b_q10_t.rd>
      <!-- upper and lower temperatures for Cox temperature scaling for temperature sensitive parameters -->
      <tupp_cox.vcmax>36</tupp_cox.vcmax>
      <tupp_cox.rd>45</tupp_cox.rd>
      <tlow_cox.vcmax>0</tlow_cox.vcmax>
      <tlow_cox.rd>5</tlow_cox.rd>
      <exp_cox.vcmax>0.3</exp_cox.vcmax>
      <exp_cox.rd>0.4</exp_cox.rd>
      <!-- gamma star quadratic leaf temperature scaling parameters -->
      <gstar_bf_a>0.012</gstar_bf_a>
      <gstar_bf_b>1.68</gstar_bf_b>
      <gstar_bf_c>42.7</gstar_bf_c>
      <!-- universal gas constant -->
      <R>8.31446</R>
    </pars>
    <env>
      <!-- atmospheric O2 concentration -->
      <o2_conc>0.21</o2_conc>
      <!-- atmospheric pressure -->
      <atm_press>101325</atm_press>
    </env>
  </leaf>
</default>
